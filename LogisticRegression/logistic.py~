import numpy as np

def LogisticRegression:

    #dim is the dimensions for weight
    def __init__(self, dim):
        self.dim = dim
        self.w = np.zeros(dim+1) # for bias

    def calc(data):
        z = np.sum(self.w * data)
        return 1.0/(1.0+np.exp(-z))

    def train(dataset):
        for data in dataset:
            data
            
        
